{
  "env": {
    "browser": true,
    "es2019": true,
    "jest": true,
  },
  "extends": ["eslint:recommended", "plugin:eslint/recommended", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": ["./tsconfig.json"],
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "@import/no-anonymous-default-export": "off",
  },
}
